<Application x:Class="MAV.Login.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:MAV.Login"
             xmlns:ui="http://schemas.modernwpf.com/2019"
             StartupUri="Login.xaml">
  <Application.Resources>
    <ResourceDictionary>
      <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="Theme/NavigationButtonTheme.xaml"/>
        <ResourceDictionary Source="Theme/WindowSizeButtonsTheme.xaml"/>
        <ui:ThemeResources RequestedTheme="Light" AccentColor ="DarkOrange" />
        <ui:XamlControlsResources/>
      </ResourceDictionary.MergedDictionaries>
    </ResourceDictionary>
  </Application.Resources>
</Application>
